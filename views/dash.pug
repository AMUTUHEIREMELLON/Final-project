doctype html
head
  meta(charset='UTF-8')
  title Parking Dashboard
  link(rel='stylesheet' href='public/css/dash.css')
  link(href='https://unpkg.com/boxicons@2.1.4/css/boxicons.min.css' rel='stylesheet')
  link(rel='stylesheet' href='/css/dash.css')
  meta(name='viewport' content='width=device-width, initial-scale=1.0')
.sidebar
  .logo-details
    span.logo_name DOCKVILLE
  ul.nav-links
    li
      a.active(href='#')
        i.bx.bx-grid-alt
        span.links_name Dashboard
    
    li
      a(href='/api/carlist')
        i.bx.bx-list-ul
        span.links_name Vehicle list
    li
      a(href='/api/car')
        i.bx.bx-coin-stack
        span.links_name Register new car
    li
      a(href='/api/boda')
        i.bx.bx-coin-stack
        span.links_name Register Boda
    li
      a(href='/api/list')
        i.bx.bx-list-ul
        span.links_name Boda List
    //- li
    //-   a(href='#')
    //-     i.bx.bx-cog
    //-     span.links_name Setting
    li.log_out
      a(href='/api/logout')
        i.bx.bx-log-out
        span.links_name Log out
section.home-section
  nav
    .sidebar-button
      i.bx.bx-menu.sidebarBtn
      span.dashboard Parking
    .search-box
      input(type='text' placeholder='Search...')
      i.bx.bx-search
    .profile-details
      img(src='/images/mln.jpg' alt='')
      span.admin_name Mellon Amullon
      i.bx.bx-chevron-down
  .home-content
    .overview-boxes
      .box
        .right-side
          a(href='/api/index') 
            
            span.text Home
       
      .box
        .right-side
          .box-topic 
          a(href='/api/car') Register car
          //- .number Shs30000
          //- .indicator
            
            span.text From morning
       
      //- .box
      //-   .right-side
      //-     .box-topic S
      //-     .number 
      //-     .indicator
            
      //-       span.text 
      
    .sales-boxes
      .recent-sales.box
        .title Recent 
        .sales-details
          ul.details
            li.topic Date
            li
              a(href='#') 02 June 2023
            li
              a(href='#') 02 June 2023
           
          ul.details
            li.topic Customer
            li
              a(href='#') Mellon	Amutuheire
            li
              a(href='#') Dave	Rizbon
            
          ul.details
            li.topic Slots
            li
              a(href='#') 01
            li
              a(href='#') 02
            
          ul.details
            li.topic Cost
            li
              a(href='#') Shs.3000
            li
              a(href='#') Shs.5000
           
        .button
          a(href='/api/carlist') See All
      .top-sales.box
        .title Other sections
        ul.top-sales-details
          li
            a(href='/api/battery')
              img(src='/images/battery.webp' alt='')
              span.product  Battery section
            
          li
            a(href='/api/tyre')
              img(src='/images/tire.jpg' alt='')
              span.product Tyre clinic section
         
          
